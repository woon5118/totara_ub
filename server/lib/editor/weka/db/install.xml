<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="lib/db" VERSION="20200131" COMMENT="XMLDB file for weka editor tables"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="editor_weka_emojis" COMMENT="Emoji details">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="name" TYPE="char" LENGTH="50" NOTNULL="true" SEQUENCE="false" COMMENT="Name of the Emoji"/>
        <FIELD NAME="category" TYPE="char" LENGTH="50" NOTNULL="true" SEQUENCE="false" COMMENT="Category of the Emoji"/>
        <FIELD NAME="pattern" TYPE="char" LENGTH="50" NOTNULL="true" SEQUENCE="false" COMMENT="Pattern match"/>
        <FIELD NAME="shortcode" TYPE="char" LENGTH="10" NOTNULL="true" SEQUENCE="false" COMMENT="Shortcode"/>
        <FIELD NAME="active" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" COMMENT="Emoji active or not"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>
