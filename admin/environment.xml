    <PHP version="5.6.5" level="required">
    </PHP>
    <PCREUNICODE level="optional">
      <FEEDBACK>
        <ON_CHECK message="pcreunicodewarning" />
      </FEEDBACK>
    </PCREUNICODE>
    <PHP_EXTENSIONS>
