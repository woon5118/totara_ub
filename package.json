{
    "name": "totaraenterprise",
    "private": true,
    "description": "Totara Enterprise",
    "scripts": {
        "tui-build": "node ./client/tooling/tasks/build.js --mode production --mode development",
        "tui-build-prod": "node ./client/tooling/tasks/build.js --mode production",
        "tui-build-dev": "node ./client/tooling/tasks/build.js --mode development",
        "tui-build-watch": "node ./client/tooling/tasks/build.js --mode development --watch",
        "tui-style-check": "node ./client/tooling/tasks/lint.js --formatting",
        "tui-style-fix": "node ./client/tooling/tasks/lint.js --fix --formatting",
        "tui-style-format": "node ./client/tooling/tasks/prettier.js --write",
        "tui-test": "npm run --silent tui-test-modern && npm run --silent tui-test-legacy",
        "tui-test-modern": "cross-env NODE_OPTIONS='--unhandled-rejections=strict' jest",
        "tui-test-legacy": "cross-env NODE_OPTIONS='--unhandled-rejections=strict' BABEL_LEGACY=1 jest",
        "tui-test-watch": "jest --watch",
        "tui-ci": "npm run tui-build && npm run tui-test && npm run tui-style-check",
        "tui-clean": "node ./client/tooling/tasks/clean.js",
        "setup-hooks": "node ./client/tooling/tasks/setup-hooks.js",
        "remove-hooks": "node ./client/tooling/tasks/remove-hooks.js",
        "generate-configs": "node ./client/tooling/tasks/generate-configs.js",
        "tui-init": "node ./client/tooling/tasks/tui-init.js"
    },
